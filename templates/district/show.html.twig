{% extends 'base.html.twig' %}

{% block title %}District{% endblock %}

{% block body %}
    <h1> <small> District </small> </h1>

    <table class="table table-striped table-hover">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ district.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ district.name }}</td>
            </tr>

            <tr>  <td colspan="2"> </td>  </tr>

            <tr>
                <th>Longitude</th>
                <th>Latitude</th>
            </tr>
            <tr>
                <td>{{ district.longitude }}</td>
                <td>{{ district.latitude }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn-default" href="{{ path('app_district_index') }}" role="button"> Retour </a>

    <a class="btn btn-default" href="{{ path('app_district_edit', {'id': district.id}) }}" role="button"> Edit</a>

    {{ include('district/_delete_form.html.twig') }}

{% endblock %}
