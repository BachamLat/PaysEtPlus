{% extends 'base.html.twig' %}

{% block title %}Municipality{% endblock %}

{% block body %}
    <h1>Municipality</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ municipality.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ municipality.name }}</td>
            </tr>
            <tr>
                <th>Longitude</th>
                <td>{{ municipality.longitude }}</td>
            </tr>
            <tr>
                <th>Latitude</th>
                <td>{{ municipality.latitude }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_municipality_index') }}">back to list</a>

    <a href="{{ path('app_municipality_edit', {'id': municipality.id}) }}">edit</a>

    {{ include('municipality/_delete_form.html.twig') }}
{% endblock %}
